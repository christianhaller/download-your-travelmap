{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-deno@1.1.1"
    }
  },
  "rewrites": [
    { "source": "/api/highscore/alltime", "destination": "/api/highscore?alltime" }
  ],
  "build": {
    "env": {
      "DENOVERSION": "1.16.1"
    }
  },
  "github": {
    "enabled": true
  },
  "public": true
}
